{
  "name": "Octanetopus",
  "version": "1.0.4",
  "description": "Octane Chrome Extension",
  "manifest_version": 2,
  "icons": {
    "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost:9090/ui/*"
      ],
      "css": [
        "/content/octanetopus-content.css"
      ],
      "js": [
        "/content/octanetopus-content.js"
      ]
    }
  ]
}
